    </interpolation>
    <center>
      <x-m>0.23</x-m>
      <y-m>-0.14</y-m>
      <z-m>-0.43</z-m>
    </center>
    <axis>
      <x>1.0</x>
      <y>0.0</y>
      <z>-0.216</z>
    </axis>
  </animation>

