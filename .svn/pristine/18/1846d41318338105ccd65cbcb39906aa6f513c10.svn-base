    <object-name>FUS_Nose_WWell</object-name>
    <condition>
      <or>
        <greater-than>
          <property>fdm/jsbsim/gear/unit[0]/pos-norm-real</property>
          <value>0.0</value>
        </greater-than>
        <greater-than>
          <property>fdm/jsbsim/gear/unit[0]/torn</property>
          <value>0.0</value>
        </greater-than>
      </or>
    </condition>
