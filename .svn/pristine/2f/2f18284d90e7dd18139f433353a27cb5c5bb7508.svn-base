    <vtailarea unit="FT2">              43.06 </vtailarea>
    <vtailarm  unit="FT" >              14.485 </vtailarm>
    <location name="AERORP" unit="M">
      <x>   4.022 </x>
      <y>   0.00 </y>
      <z>   0.00 </z>
    </location>
