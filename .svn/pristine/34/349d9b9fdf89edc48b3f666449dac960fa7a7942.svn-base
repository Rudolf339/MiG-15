<animation>
<type>material</type>
<object-name>TRM_Base</object-name>
<object-name>TRM_Bolts</object-name>
<object-name>TRM_ABase</object-name>
<object-name>TRM_EBase</object-name>
<object-name>TRM_AMessage</object-name>
<object-name>TRM_EMessage</object-name>
<object-name>TRM_Aileron</object-name>
<object-name>TRM_Elevator</object-name>
<emission>
<factor-prop>systems/light/canopy-lamps-norm</factor-prop>
<red>0.777</red>
<green>0.700</green>
<blue>0.700</blue>
</emission>
<condition>
