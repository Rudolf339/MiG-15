</equals>
</condition>
</animation>

  -->

  <animation>
    <type>rotate</type>
    <object-name>TRM_Aileron</object-name>
    <object-name>TRM_ABolt</object-name>
    <property>controls/flight/aileron-trim</property>
    <interpolation>
      <entry><ind>-1</ind><dep>-36</dep></entry>
      <entry><ind>1</ind><dep>142</dep></entry>
    </interpolation>
    <center>
      <x-m>-0.032</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>0.0</x>
      <y>0.0</y>
      <z>-1</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>TRM_Elevator</object-name>
    <object-name>TRM_EBolt</object-name>
    <property>controls/flight/elevator-trim</property>
    <interpolation>
      <entry><ind>-1</ind><dep>-36</dep></entry>
      <entry><ind>1</ind><dep>142</dep></entry>
    </interpolation>
    <center>
      <x-m>0.032</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>0.0</x>
      <y>0.0</y>
      <z>-1</z>
    </axis>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>TRM_EDown</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>controls/flight/elevator-trim</property>
        <step>-0.02</step>
        <min>-1</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <action>
      <button>1</button>
      <repeatable>true</repeatable>
      <interval-sec>0.1</interval-sec>
