<?xml version="1.0"?>

<PropertyList>

  <path>MiG-15bis-Stopcontrol.ac</path> 

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>30</max-m>
  </animation>

  <!--

<animation>
<type>material</type>
<object-name>PBC_Lever</object-name>
<object-name>PBC_LRing</object-name>
<object-name>PBC_LBase</object-name>
<object-name>PBC_Up</object-name>
<object-name>PBC_Down</object-name>
<emission>
<factor-prop>systems/light/canopy-lamps-norm</factor-prop>
<red>0.777</red>
<green>0.700</green>
<blue>0.700</blue>
</emission>
<condition>
<equals>
<property>systems/light/use-canopy-lamps</property>
<value>1</value>
</equals>
</condition>
</animation>

  -->

  <animation>
    <type>rotate</type>
    <object-name>PBC_Lever</object-name>
    <object-name>PBC_LBase</object-name>
    <object-name>PBC_LRing</object-name>
    <property>fdm/jsbsim/systems/stopcontrol/lever-pos</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>0.0</dep></entry>
      <entry><ind>1.0</ind><dep>36.96</dep></entry>
    </interpolation>
    <center>
      <x-m>-0.085</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>  
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>PBC_Lever</object-name>
    <object-name>PBC_LRing</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>nasal</command>
        <script>MIG15BIS.bitswap("fdm/jsbsim/systems/stopcontrol/lever-input")</script>
      </binding>
    </action>
  </animation>

</PropertyList> 
