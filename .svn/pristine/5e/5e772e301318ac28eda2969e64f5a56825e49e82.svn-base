<?xml version="1.0"?>

<PropertyList>

  <path>MiG-15bis-Bustercontrol.ac</path> 

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>30</max-m>
  </animation>

  <!--
      <animation>
      <type>material</type>
      <object-name>BUC_Handle</object-name>
      <object-name>BUC_Base</object-name>
      <object-name>BUC_Fix</object-name>
      <object-name>BUC_Lever</object-name>
      <object-name>BUC_Bolts</object-name>
      <object-name>BUC_Down</object-name>
      <object-name>BUC_Up</object-name>
      <emission>
      <factor-prop>systems/light/canopy-lamps-norm</factor-prop>
      <red>0.777</red>
      <green>0.700</green>
      <blue>0.700</blue>
      </emission>
      <condition>
      <equals>
      <property>systems/light/use-canopy-lamps</property>
      <value>1</value>
      </equals>
      </condition>
      </animation>
  -->

  <animation>
    <type>translate</type>
    <object-name>BUC_Fix</object-name>
    <property>fdm/jsbsim/systems/boostercontrol/fix-pos</property>
    <interpolation>
      <entry><ind>1.0</ind><dep>0.0</dep></entry>
      <entry><ind>0.0</ind><dep>0.004</dep></entry>
    </interpolation>
    <axis>
      <x>0.0</x>
      <y>0</y>
      <z>-1.0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>BUC_Lever</object-name>
    <object-name>BUC_Handle</object-name>
    <object-name>BUC_Fix</object-name>
    <property>fdm/jsbsim/systems/boostercontrol/control-pos</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>-34</dep></entry>
      <entry><ind>1.0</ind><dep>34</dep></entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>-0.085</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>BUC_Down</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>fdm/jsbsim/systems/boostercontrol/control-input</property>
        <step>-0.5</step>
        <min>0</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <action>
      <button>1</button>
      <repeatable>false</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>fdm/jsbsim/systems/boostercontrol/control-input</property>
        <step>-0.5</step>
        <min>0</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>BUC_Up</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>fdm/jsbsim/systems/boostercontrol/control-input</property>
        <step>0.5</step>
        <min>0</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <action>
      <button>1</button>
      <repeatable>false</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>fdm/jsbsim/systems/boostercontrol/control-input</property>
        <step>0.5</step>
        <min>0</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

</PropertyList> 
