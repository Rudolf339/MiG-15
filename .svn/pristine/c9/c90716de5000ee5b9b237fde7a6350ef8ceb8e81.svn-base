<?xml version="1.0"?>

<PropertyList>

  <path>MiG-15bis-Gascontrol.ac</path> 

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>30</max-m>
  </animation>

  <!--

<animation>
<type>material</type>
<object-name>GAC_Lever</object-name>
<object-name>GAC_Handle</object-name>
<object-name>GAC_Fix</object-name>
<object-name>GAC_LBase</object-name>
<object-name>GAC_RBase</object-name>
<object-name>GAC_Down</object-name>
<object-name>GAC_Up</object-name>
<object-name>GAC_StopNear</object-name>
<object-name>GAC_SNBolt</object-name>
<object-name>GAC_StopFar</object-name>
<object-name>GAC_SFBolt</object-name>
<object-name>GAC_RBFastNear</object-name>
<object-name>GAC_NearBolts</object-name>
<object-name>GAC_RBFastFar</object-name>
<object-name>GAC_FarBolts</object-name>
<object-name>GB_Button</object-name>
<object-name>GB_Bezel</object-name>
<object-name>GB_Base</object-name>
<object-name>GB_Fix</object-name>
<object-name>RB_Button</object-name>
<object-name>RB_Bezel</object-name>
<object-name>GAC_Lock</object-name>
<object-name>GAC_LoBase</object-name>
<emission>
<factor-prop>systems/light/canopy-lamps-norm</factor-prop>
<red>0.777</red>
<green>0.700</green>
<blue>0.700</blue>
</emission>
<condition>
<equals>
<property>systems/light/use-canopy-lamps</property>
<value>1</value>
</equals>
</condition>
</animation>

  -->

  <animation>
    <type>rotate</type>
    <object-name>GAC_Lever</object-name>
    <object-name>GAC_Fix</object-name>
    <object-name>GAC_Handle</object-name>
    <object-name>GB_Base</object-name>
    <object-name>GB_Fix</object-name>
    <object-name>GB_Bezel</object-name>
    <object-name>GB_Button</object-name>
    <object-name>RB_Bezel</object-name>
    <object-name>RB_Button</object-name>
    <property>fdm/jsbsim/systems/gascontrol/lever-pos</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>-24.75</dep></entry>
      <entry><ind>1.0</ind><dep>24.75</dep></entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>-0.085</z-m>
    </center>  
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>GB_Button</object-name>
    <property>instrumentation/gas-control/brakes-button/switch-pos-norm</property>
    <factor>0.003</factor>
    <axis>
      <x>0.0</x>
      <y>0</y>
      <z>-1</z>
    </axis>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>GB_Button</object-name>
    <object-name>GB_Fix</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>instrumentation/gas-control/brakes-button/target-pos-norm</property>
      </binding>
      <binding>
        <command>property-interpolate</command>
        <property>instrumentation/gas-control/brakes-button/switch-pos-norm</property>
        <property>instrumentation/gas-control/brakes-button/target-pos-norm</property>
        <time>0.3</time>
      </binding>
      <binding>
        <command>nasal</command>
        <script>MIG15BIS.clicksound();</script>
      </binding>
    </action>
  </animation>


  <animation>
    <type>rotate</type>
    <object-name>GAC_Fix</object-name>
    <property>fdm/jsbsim/systems/gascontrol/fix-pos</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>0.0</dep></entry>
      <entry><ind>1.0</ind><dep>90</dep></entry>
    </interpolation>
    <center>
      <x-m>0.009</x-m>
      <y-m>-0.005</y-m>
      <z-m>0.070</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>GAC_Lock</object-name>
    <property>fdm/jsbsim/systems/gascontrol/lock-pos</property>
    <interpolation>
      <entry><ind>1.0</ind><dep>45</dep></entry>
      <entry><ind>0.0</ind><dep>0</dep></entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>-0.085</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>GAC_Down</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine/throttle</property>
        <step>-0.01</step>
        <min>0</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <action>
      <button>1</button>
      <repeatable>true</repeatable>
      <interval-sec>0.05</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine/throttle</property>
        <step>-0.1</step>
        <min>0</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>GAC_Up</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine/throttle</property>
        <step>0.01</step>
        <min>0</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <action>
      <button>1</button>
      <repeatable>true</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine/throttle</property>
        <step>0.05</step>
        <min>0</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>


  <animation>
    <type>pick</type>
    <object-name>GAC_Lock</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>nasal</command>
        <script>MIG15BIS.bitswap("fdm/jsbsim/systems/gascontrol/lock-command")</script>
      </binding>

    </action>
    <action>
      <button>1</button>
      <repeatable>false</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>nasal</command>
        <script>MIG15BIS.bitswap("fdm/jsbsim/systems/gascontrol/lock-command")</script>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>GAC_Fix</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>nasal</command>
        <script>MIG15BIS.bitswap("fdm/jsbsim/systems/gascontrol/fix-command")</script>
      </binding>
    </action>
    <action>
      <button>1</button>
      <repeatable>false</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>nasal</command>
        <script>MIG15BIS.bitswap("fdm/jsbsim/systems/gascontrol/fix-command")</script>
      </binding>
    </action>
  </animation>


</PropertyList> 
