<?xml version="1.0"?>

<PropertyList>

  <path>MiG-15bis-Horizon.ac</path> 

  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>HO_Glass</object-name>
  </effect>

  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>HO_Tube</object-name>
  </effect>

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>30</max-m>
  </animation>

  <animation>
    <type>material</type>
    <object-name>HO_Sphere</object-name>
    <object-name>HO_Backpanel</object-name>
    <object-name>HO_Backplate</object-name>
    <object-name>HO_Ball</object-name>
    <object-name>HO_Tube</object-name>
    <object-name>HO_Ring</object-name>
    <emission>
      <factor-prop>systems/electrical-real/outputs/horizon/volts-norm</factor-prop>
      <red>0.222</red>
      <green>0.212</green>
      <blue>0.212</blue>
    </emission>
  </animation>

  <!--

<animation>
<type>material</type>
<object-name>HO_Sphere</object-name>
<object-name>HO_Backpanel</object-name>
<object-name>HO_Backplate</object-name>
<object-name>HO_Ball</object-name>
<object-name>HO_Tube</object-name>
<object-name>HO_Ring</object-name>
<object-name>HO_Vern</object-name>
<object-name>HO_Button</object-name>
<emission>
<factor-prop>systems/electrical-real/outputs/horizon/volts-norm</factor-prop>
<red>0.777</red>
<green>0.700</green>
<blue>0.700</blue>
</emission>
<condition>
<equals>
<property>systems/light/use-canopy-lamps</property>
<value>1</value>
</equals>
</condition>
</animation>

  -->


  <animation>
    <type>rotate</type>
    <object-name>HO_Ball</object-name>
    <property>instrumentation/slip-skid-ball/indicated-slip-skid</property>
    <min>-1</min>
    <max>1</max>
    <interpolation>
      <entry><ind>-1</ind><dep>-7.8</dep></entry>
      <entry><ind>1</ind><dep>7.8</dep></entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0.0122</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>HO_Sphere</object-name>
    <property>fdm/jsbsim/systems/arthorizon/roll-indicated-deg</property>
    <center>
      <x-m>-0.054</x-m>
      <y-m>0</y-m>
      <z-m>0.009</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>HO_Sphere</object-name>
    <property>fdm/jsbsim/systems/arthorizon/pitch-indicated-deg</property>
    <center>
      <x-m>-0.054</x-m>
      <y-m>0</y-m>
      <z-m>0.009</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>HO_Button</object-name>
    <property>fdm/jsbsim/systems/arthorizon/button-pos</property>
    <factor>0.008</factor>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>HO_Ring</object-name>
    <property>fdm/jsbsim/systems/arthorizon/offset</property>
    <factor>0.015</factor>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>HO_Vern</object-name>
    <property>fdm/jsbsim/systems/arthorizon/offset</property>
    <factor>360</factor>
    <center>
      <x-m>0</x-m>
      <y-m>-0.063</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>HO_Down</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>fdm/jsbsim/systems/arthorizon/offset</property>
        <step>-0.02</step>
        <min>-1</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <action>
      <button>1</button>
      <repeatable>true</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>fdm/jsbsim/systems/arthorizon/offset</property>
        <step>-0.1</step>
        <min>-1</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>HO_Up</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>fdm/jsbsim/systems/arthorizon/offset</property>
        <step>0.02</step>
        <min>-1</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <action>
      <button>1</button>
      <repeatable>true</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>fdm/jsbsim/systems/arthorizon/offset</property>
        <step>0.1</step>
        <min>-1</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>HO_Button</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>nasal</command>
        <script>MIG15BIS.bitswap("fdm/jsbsim/systems/arthorizon/button-command")</script>
      </binding>
    </action>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>nasal</command>
        <script>MIG15BIS.bitswap("fdm/jsbsim/systems/arthorizon/button-command")</script>
      </binding>
    </action>
  </animation>


</PropertyList> 
