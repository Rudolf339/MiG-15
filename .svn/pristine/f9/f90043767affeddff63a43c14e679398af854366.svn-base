<?xml version="1.0"?>

<PropertyList>

  <submodel>
    <name>Trajectory marker</name>
    <model>Aircraft/MiG-15/Models/Submodels/Trajectory-Marker.xml</model>
    <trigger>ai/submodels/trajectory-markers</trigger>
    <speed>0</speed>
    <repeat>true</repeat>
    <delay>1.0</delay>
    <count>-1</count>
    <x-offset>-20.0</x-offset>
    <y-offset>0</y-offset>
    <z-offset>0.10</z-offset>
    <yaw-offset>0.0</yaw-offset>
    <pitch-offset>0.0</pitch-offset>
    <eda>1000000000.00</eda>
    <life>900</life>
    <buoyancy>32</buoyancy>
    <wind>false</wind>
    <aero-stabilised>false</aero-stabilised>
  </submodel>

  <submodel>
    <name>N-37 round</name>
    <model>Aircraft/MiG-15/Models/Submodels/MiG-15bis-N-37-round.xml</model>
    <trigger>ai/submodels/N-37</trigger>
    <speed>2260</speed>
    <repeat>true</repeat>
