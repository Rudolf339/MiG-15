    <center>
      <x-m>0.022</x-m>
      <y-m>0.0</y-m>
      <z-m>0.0</z-m>
    </center>  
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>PUTT_Switch</object-name>
    <property>fdm/jsbsim/systems/leftpanel/third-tank-pump-pos</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>45.0</dep></entry>
      <entry><ind>1.0</ind><dep>-45.0</dep></entry>
    </interpolation>
    <center>
      <x-m>-0.053</x-m>
      <y-m>0.0</y-m>
      <z-m>0.0</z-m>
    </center>  
    <axis>
