    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>IGNT_Message</object-name>
    <object-name>IGNT_Switch</object-name>
    <object-name>IGNT_SForeplate</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>nasal</command>
        <script>MIG15BIS.bitswap("fdm/jsbsim/systems/leftpanel/ignition-type-input")</script>
      </binding>
    </action>
    <action>
      <button>1</button>
      <repeatable>false</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>nasal</command>
        <script>MIG15BIS.bitswap("fdm/jsbsim/systems/leftpanel/ignition-type-input")</script>
      </binding>
    </action>
  </animation>

