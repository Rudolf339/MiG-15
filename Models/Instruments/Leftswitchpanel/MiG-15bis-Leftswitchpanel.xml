  <animation>
    <type>material</type>
    <object-name>EBC_Lamp</object-name>
    <object-name>EBC_Glass</object-name>
    <emission>
      <factor-prop>systems/electrical-real/outputs/isolation-lamp/volts-norm</factor-prop>
      <red>0.9</red>
      <green>0.1</green>
      <blue>0.1</blue>
    </emission>
  </animation>


  <animation>
    <type>rotate</type>
    <object-name>IGNT_Switch</object-name>
    <property>fdm/jsbsim/systems/leftpanel/ignition-type-pos</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>45.0</dep></entry>
      <entry><ind>1.0</ind><dep>-45.0</dep></entry>
    </interpolation>
    <center>
      <x-m>0.093</x-m>
      <y-m>0.017</y-m>
      <z-m>0.0</z-m>
    </center>  
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>PUC_Switch</object-name>
    <property>fdm/jsbsim/systems/leftpanel/pump-pos</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>45.0</dep></entry>
      <entry><ind>1.0</ind><dep>-45.0</dep></entry>
    </interpolation>
    <center>
      <x-m>0.022</x-m>
      <y-m>-0.014</y-m>
      <z-m>0.0</z-m>
    </center>  
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>EBC_Switch</object-name>
    <property>fdm/jsbsim/systems/leftpanel/isolation-valve-pos</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>45.0</dep></entry>
      <entry><ind>1.0</ind><dep>-45.0</dep></entry>
    </interpolation>
    <center>
      <x-m>0.022</x-m>
      <y-m>0.0</y-m>
      <z-m>0.0</z-m>
    </center>  
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>IGN_Switch</object-name>
    <property>fdm/jsbsim/systems/leftpanel/ignition-pos</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>45.0</dep></entry>
